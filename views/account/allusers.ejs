<% include ../header %>

<table class="table table-striped table-hover usersTable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i = 0; i < users.length; i++) {%>
      <tr>
          <td>
            <%= users[i]["firstname"] %> <%= users[i]["lastname"] %>
          </td>
          <td>
            <%= users[i]["username"] %>
          </td>
      </tr>
    <% } %>
  </tbody>
</table>

<% include ../footer %>